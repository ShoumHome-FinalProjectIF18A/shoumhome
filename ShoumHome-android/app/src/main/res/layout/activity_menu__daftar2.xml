<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu_Daftar2">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/nama_slogan"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="64dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvDaftar2"
        android:layout_width="226dp"
        android:layout_height="37dp"
        android:text="Isi data dirimu sekarang!"
        android:textColor="@color/hitm"
        android:textSize="18dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="92dp"
        tools:layout_editor_absoluteY="202dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/etnamalengkap"
        android:layout_width="352dp"
        android:layout_height="51dp"
        android:ems="10"
        android:hint="Nama Lengkap"
        android:inputType="textPersonName"
        android:textAllCaps="false"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="239dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="351dp"
        android:layout_height="54dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Nomor Ponsel"
        android:textAllCaps="false"
        android:textSize="18dp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="292dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="352dp"
        android:layout_height="47dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="18dp"
        android:textAllCaps="false"
        android:hint="Tanggal Lahir"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="347dp"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="351dp"
        android:layout_height="43dp"
        android:text="Saya menyetujui Syarat dan Ketentuan yang berlaku."
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="407dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="225dp"
        android:layout_height="19dp"
        android:text="Syarat dan Ketentuan"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="485dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btn_kembali_daftar2"
        android:layout_width="152dp"
        android:layout_height="45dp"
        android:text="Kembali"
        android:textAllCaps="false"
        android:textSize="18dp"
        android:textStyle="bold"
        android:textColor="@color/warna_tulisan_button"
        android:background="@color/warna_background_button"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="575dp" />

    <Button
        android:id="@+id/btn_lanjut_daftar1"
        android:layout_width="150dp"
        android:layout_height="43dp"
        android:textAllCaps="false"
        android:textSize="18dp"
        android:textStyle="bold"
        android:textColor="@color/warna_tulisan_button"
        android:background="@color/warna_background_button"
        android:text="Lanjut"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="230dp"
        tools:layout_editor_absoluteY="577dp" />


</androidx.constraintlayout.widget.ConstraintLayout>